    (if (== size 0)
        (= (vec-s rd) (f32 (bitcast (vec-s rn) u32)))
        (= (vec-d rd) (f64 (bitcast (vec-d rn) u64)))))

(def UCVTF-vector
    "0 Q 1 01110 0 s 10000 11101 10 nnnnn ddddd"
    "ucvtf V$rd.$t, V$rn.$t"
