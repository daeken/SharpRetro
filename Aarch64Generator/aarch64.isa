                    (vector-insert rd 4 (vector-element av (+ base 2) u8))
                    (vector-insert rd 6 (vector-element av (+ base 3) u8))
                    (vector-insert rd 1 (vector-element bv (+ base 0) u8))
                    (vector-insert rd 3 (vector-element bv (+ base 1) u8))
