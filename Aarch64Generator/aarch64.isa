(def ZIP
    "0 Q 001110 ss 0 mmmmm 0 o 11 10 nnnnn ddddd"
    "zip$i V$rd.$T, V$rn.$T, V$rm.$T"
    (names (Q Q) (size s) (rm m) (op o) (rn n) (rd d))
    (let i (u8 (+ op 1))
        (let T (match (: size Q)
            0b00_0 "8B"
            0b00_1 "16B"
            0b01_0 "4H"
            0b01_1 "8H"
            0b10_0 "2S"
            0b10_1 "4S"
            0b11_1 "2D"
            (unimplemented))))
